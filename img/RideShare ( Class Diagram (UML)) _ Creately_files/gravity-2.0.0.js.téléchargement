var envConfig = __webpack_require__(2);
var token_1 = __webpack_require__(15);
var GravityCookie = /** @class */ (function () {
    function GravityCookie() {
        this.GRAVITY_COOKIE_NAME = 'gravity_token';
    }
    /**
     * Return a GravityCookie instance
     * @return GravityCookie instance
     */
    GravityCookie.create = function () {
        if (!this._gravityCookieInstance) {
            this._gravityCookieInstance = new GravityCookie();
        }
        return this._gravityCookieInstance;
    };
    /**
     * Read browser cookies and exctract Gravity authetication token
     * will return empty string if Gravity cookie is not present
     * @return Gravity auth token read from cookies
     */
    GravityCookie.prototype.getCookie = function () {
        var value = '; ' + document.cookie;
        var parts = value.split('; ' + this.GRAVITY_COOKIE_NAME + '=');
        var cookieValue = '';
        if (parts.length == 2) {
            cookieValue = parts.pop().split(';').shift();
        }
        return cookieValue;
    };
    /**
     * Save Gravity token cookie
     * @param token Gravity token string
     */
    GravityCookie.prototype.setCookie = function (token) {
        if (token) {
            document.cookie =
                this.GRAVITY_COOKIE_NAME +
                    '=' +
                    token +
                    '; Domain=' +
                    this.getCookieDomain() +
                    '; Path=/; Expires=' +
                    this.getUTCStringFromTimeStamp(token);
        }
    };
    /**
     * Remove current Gravity token cookie, when called current user session will be removed and
     * user falls into unautheticated state
     */
    GravityCookie.prototype.removeCookie = function () {
        document.cookie =
            this.GRAVITY_COOKIE_NAME +
                '=; path=/; domain=' +
                this.getCookieDomain() +
                '; expires=' +
                new Date(0).toUTCString();
    };
    /**
     * Return the Gravity cookie domain, this is useful when setting/unsetting cookies to
     * determine the domain to access the cookie
     * @return Gravity cookie domain
     */
    GravityCookie.prototype.getCookieDomain = function () {
        return envConfig.cookie_domain;
    };
    /**
     * Return human readable time stamp string from the unix timestamp of the Gravity token
     * expiration value
     * @param token Gravity token string
     * @return string containing the UTC timestamp
     */
    GravityCookie.prototype.getUTCStringFromTimeStamp = function (token) {
        var tokenExpiryDate = token_1.Token.create(token).getExpirationTime();
        return new Date(tokenExpiryDate * 1000).toUTCString();
    };
    /**
     * Save given Gravity token string as a cookie
     * @param token Gravity token string
     */
    GravityCookie.prototype.set = function (token) {
        this.setCookie(token);
    };
    /**
     * Return current browser Gravity token cookie value
     * @return Gravity token value
     */
    GravityCookie.prototype.get = function () {
        return this.getCookie();
    };
    /**
     * Unset current Gravity token cookie if exists
     */
    GravityCookie.prototype.remove = function () {
        this.removeCookie();
    };
    /**
     * Return Gravity cookie domain
     * @return Gravity cookie domain
     */
    GravityCookie.prototype.getDomain = function () {
        return this.getCookieDomain();
    };
    return GravityCookie;
}());
exports.GravityCookie = GravityCookie;
