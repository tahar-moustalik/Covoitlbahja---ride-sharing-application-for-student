     */
    Client.prototype.getCreatelyHome = function () {
        return envConfig.creately_home;
    };
    /**
     * Return current Gravity authentication cookie value
     */
    Client.prototype.getCookie = function () {
        return gravity_cookie_1.GravityCookie.create().get();
    };
    return Client;
