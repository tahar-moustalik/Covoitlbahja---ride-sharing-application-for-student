var unicodeRegex = __webpack_require__(58);
/**
 * class Validator
 *
 * Provide validation functionality for user inputs required for Gravity
 */
var Validator = /** @class */ (function () {
    function Validator() {
        // tslint:disable-next-line:max-line-length
        this.EMAIL_VALIDATION_REGEX = new RegExp(/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/);
        this.PASSWORD_VALIDATION_REGEX = new RegExp(/^(?=.*[A-Z])(?=.*\d)(?=.*(_|[^\w])).+$/);
        this.MINIMAL_PASSWORD_LENGTH = 2;
        this.MINIMUM_PASSWORD_LENGTH = 7;
        this.NAME_VALIDATION_REGEX = unicodeRegex('^\\p{L}[\\p{L} \']*$');
    }
    /**
     * Return current or new validator instance
     * @return Validator instance
     */
    Validator.create = function () {
        if (!this._validatorInstance) {
            this._validatorInstance = new Validator();
        }
        return this._validatorInstance;
    };
    /**
     * Check if a given string passes a given regular expression
     * @param parameter String to be checked against the regex
     * @param regex Regular expression
     * @return boolean indicating the string passes the regex or not
     */
    Validator.prototype.isValidByRegex = function (parameter, regex) {
        return regex.test(parameter);
    };
    /**
     * Check string length validation
     * @param parameter String to be checked
     * @param length Minimum charactors required to be valid
     * @return boolean indicating the status of the check
     */
    Validator.prototype.isValidByLength = function (parameter, length) {
        return parameter.length >= length;
    };
    /**
     * Validates an email address for RFC complience
     * @param email email to be validated
     * @return boolean true if valid, else string message
     */
    Validator.prototype.isValidEmail = function (email) {
        if (!Boolean(email)) {
            return 'Email address is required.';
        }
        else if (!this.isValidByRegex(email, this.EMAIL_VALIDATION_REGEX)) {
            return 'Email address is invalid.';
        }
        else {
            return true;
        }
    };
    /**
     * Check if a given password string is ecceptable to perform Authetication
     * Note that this function need to be used only during authetication process.
     * This is required to backword compatibility with pre-Gravity users
     * @param password password string provided by user during Authentication
     */
    Validator.prototype.isMinimalValidPassword = function (password) {
        if (!Boolean(password)) {
            return 'Password is required.';
        }
        else if (!this.isValidByLength(password, this.MINIMAL_PASSWORD_LENGTH)) {
            return 'Password should contain at least ' + this.MINIMAL_PASSWORD_LENGTH + ' characters.';
        }
        else {
            return true;
        }
    };
    /**
     * Check if a given password is valid by confirming to standard imposed by
     * Gravity. To be a valid password, it should
     * - contain al least one CAPITAL letter
     * - contain at least one number
     * - contain at least one special charactor
     * - charactor count should be at least 7
     * return true if valid, return a error message if invalid
     * @param password password string provided by user during Registration
     */
    Validator.prototype.isValidPassword = function (password) {
        if (!Boolean(password)) {
            return 'Password is required.';
        }
        else if (!this.isValidByLength(password, this.MINIMUM_PASSWORD_LENGTH)) {
            return 'Password should contain at least ' + this.MINIMUM_PASSWORD_LENGTH + ' characters.';
        }
        else if (!this.isValidByRegex(password, this.PASSWORD_VALIDATION_REGEX)) {
            return 'Password must contain at least one upper case, one number and one special character.';
        }
        else {
            return true;
        }
    };
    /**
     * Check if a given name string is valid
     * @param name string to be validated
     */
    Validator.prototype.isValidName = function (name) {
        if (!Boolean(name)) {
            return 'Your name is required.';
        }
        else if (!this.isValidByRegex(name, this.NAME_VALIDATION_REGEX)) {
            return 'Your name cannot contain special characters.';
        }
        else {
            return true;
        }
    };
    return Validator;
}());
exports.Validator = Validator;
