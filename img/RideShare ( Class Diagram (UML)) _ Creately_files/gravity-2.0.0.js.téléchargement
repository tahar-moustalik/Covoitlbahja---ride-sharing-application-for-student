 * class Client
 * Provide abstract functionality for any thirdparty using the Gravity js client
 */
var Client = /** @class */ (function () {
    function Client() {
    }
    /**
     * Return current Client instance
     * @return Client instance
     */
    Client.create = function () {
        if (!this._clientInstance) {
            this._clientInstance = new Client();
        }
        return this._clientInstance;
    };
    /**
     * Check if current session has a valid Gravity authentication set or not
     * @return boolean indicating the session state
     */
    Client.prototype.isValidSession = function () {
        var token = gravity_cookie_1.GravityCookie.create().get();
        return Boolean(token) && token_1.Token.create(token).isValid();
    };
    /**
     * check if current session has valid authentication or not,
     * this function can be used to identify that if the user is authenticated of need to be authenticated
     * @return boolean current session state
     */
    Client.prototype.hasValidSession = function () {
        return this.isValidSession();
    };
    /**
     * Invalidate the current Gravity session,
     * when called this remove current Gravity authentication cookie
     * making the session as unauthenticated
     */
    Client.prototype.logOut = function () {
        gravity_cookie_1.GravityCookie.create().remove();
    };
    /**
     * Return the cookie domain which the Gravity auth token cookie is set.
     * This helper can be used by any thirdparty to properly identify the cookie domain
     * so they can set, access and update cookies.
     * Note: any modifications to the Gravity authentication to need to be handled by this
     * client library itself and any thirdparty modifications to the cookie will not be tolerated.
     * @return cookie domain
     */
    Client.prototype.getCookieDomain = function () {
        return gravity_cookie_1.GravityCookie.create().getDomain();
    };
