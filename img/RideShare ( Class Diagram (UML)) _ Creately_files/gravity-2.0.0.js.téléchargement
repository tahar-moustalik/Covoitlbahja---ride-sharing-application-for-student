/**
 * class Hash
 * provide functionality to hash strings to Gravity hashing format. This hashes
 * supposed to be used during password transmission during user authentication
 */
var Hash = /** @class */ (function () {
    function Hash() {
    }
    /**
     * Return a Hash instance, if an instance is created already, will just return it
     * @return Hash instance
     */
    Hash.create = function () {
        if (!this._hashInstance) {
            this._hashInstance = new Hash();
        }
        return this._hashInstance;
    };
    /**
     * Hashes a given string using MD5
     * @param plainString string to be hased to a md5 string
     * @return md5 string
     */
    Hash.prototype.md5 = function (plainString) {
        return md5(plainString);
    };
    /**
     * Hashes a given string using SHA256
     * @param plainString sting to be hashed to a sha256 string
     * @return sha256 string
     */
    Hash.prototype.sha256 = function (plainString) {
        return sha('sha256').update(plainString, 'utf8').digest('hex');
    };
    /**
     * Hashes a given string to Gravity hash format which is dual hash
     * using md5 and followed by a rehash using sha256
     * @param plainString string to be hashed to Gravity hash format
     * @return string hashed to Gravity hash format
     */
    Hash.prototype.hash = function (plainString) {
        var md5Hash = this.md5(plainString);
        return this.sha256(md5Hash);
    };
    return Hash;
}());
