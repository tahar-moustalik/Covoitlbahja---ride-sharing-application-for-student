/***/ (function(module, exports, __webpack_require__) {

"use strict";

Object.defineProperty(exports, "__esModule", { value: true });
var envConfig = __webpack_require__(2);
window.gapi = {};
/**
 * class Google
 * Provide user authentication via Google auth2 API
 */
var Google = /** @class */ (function () {
    /**
     * This trigger the loading of the Google js sdk
     */
    function Google() {
        var _this = this;
        /**
         * Google user scopes we need access to
         * Note: if new scopes are added, existing users who already authorized creately,
         * will fail to login with a scope mismatch.
         * To fix that, the user need to remove creately authorization and re-authorize.
         * So add more scopes if it is absolutely necessory.
         */
        this.scopes = 'email profile';
        this.sdkReady = new Promise(function (resolve) {
            _this.afterSDKIsReady = resolve;
        });
        var d = document;
        var js;
        var id = 'google-jssdk';
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement('script');
        js.id = id;
        js.async = true;
        js.src = 'https://apis.google.com/js/platform.js';
        d.getElementsByTagName('head')[0].appendChild(js);
        js.onload = function () {
            _this.initializeGoogle();
        };
    }
    /**
     * Initialize Google JS sdk
     * This function is the starting point of initialization process of the
     * Google js sdk. This will load client and auth2 APIs to current Google js and
     * call next funtion to complete the initialization process.
     */
    Google.prototype.initializeGoogle = function () {
        var _this = this;
        window.gapi.load('client:auth2', function () {
            _this.initAuth2API();
        });
    };
    /**
     * Initialize Google client and auth2 APIs with client id and user authorization scopes and
     * call waitForUserLogin function which watch for user login status changes.
     */
    Google.prototype.initAuth2API = function () {
        var _this = this;
        window.gapi.auth2.init({ client_id: envConfig.google.clientId, scope: this.scopes }).then(function () {
            _this.waitForUserLogin();
        });
    };
    /**
     * Listen on user login status with Google auth2 API and call updateSignIn on each
     * user status change.
     */
    Google.prototype.waitForUserLogin = function () {
        this.auth2 = window.gapi.auth2.getAuthInstance();
        // Handle the initial sign-in state.
        this.updateSignIn(this.auth2.isSignedIn.get());
        this.afterSDKIsReady();
    };
    /**
     * Listen for user sign in status and if signed in,
     * call onSignIn function with the authInstance returned by google auth2 api.
     * @param isSignedIn Google signed in user object
     */
    Google.prototype.updateSignIn = function (isSignedIn) {
        if (isSignedIn) {
            this.onSignIn(window.gapi.auth2.getAuthInstance());
        }
    };
    /**
     * Handle the user sign in status, if signed in call checkEmail to verify the
     * user email and if sign in process return an error reject the promise by
     * calling the reject function.
     * @param authResult User authentication result object
     */
    Google.prototype.onSignIn = function (authResult) {
        if (authResult.isSignedIn.get()) {
            this.checkEmail();
        }
        else {
            // There was an error, which means the user is not signed in.
            this.actionFailed('User not signed in');
        }
    };
    /**
     * Check if the user is already signed in and authorized Creately.
     * This function need to be called by the login flow to load the user.
     * If user is not signed in or not authorized the app, this will popup
     * Google authentication window to authenticate and authorize the user for Creately.
     * @param resolve Function to call after resolved
     * @param reject Function to call after any authentication problem
     */
    Google.prototype.loginUser = function (resolve, reject) {
        var _this = this;
        this.resolveMethod = resolve;
        this.rejectMethod = reject;
        if (!this.auth2.isSignedIn.get()) {
            var sign = this.auth2.signIn();
            sign.then(function (responce) {
                _this.updateSignIn(responce);
            }).catch(function (error) {
                _this.actionFailed(error.error);
            });
        }
        else {
            this.checkEmail();
        }
    };
    /**
     * Check the current sign in user with Google has an email which is verified.
     * If verified will call resolve function on the promise and othervise call reject function
     * on the promise.
     */
    Google.prototype.checkEmail = function () {
        var user = this.auth2.currentUser.get();
        if (user.getBasicProfile().getEmail()) {
            this.actionSuccess(user.getAuthResponse());
        }
        else {
            this.actionFailed('An email is required to continue, your Google account does not contain a verified email.');
        }
    };
    /**
     * Resolve the promise by sending back the Google access token of current user.
     * @param authResponse Google authentication response which has the user access token
     */
    Google.prototype.actionSuccess = function (authResponse) {
        // If the user is already signed in into the Google account,
        // Google SDK try to login the user automatically and when this happen
        // An exception will be thrown, so type check is required
        if (typeof this.resolveMethod == 'function') {
            this.resolveMethod(authResponse.access_token);
        }
    };
    /**
     * Reject the promise and send back error message
     * @param error
     */
    Google.prototype.actionFailed = function (error) {
        this.rejectMethod(error);
    };
    /**
     * Return a Promise which will be resolved after Google SDK is loaded
     */
    Google.prototype.ready = function () {
        return this.sdkReady;
    };
    /**
     * Call login function to login the user with Google.
     * Will return a promise.
     */
    Google.prototype.login = function () {
        var _this = this;
        return new Promise(function (resolve, reject) {
            _this.loginUser(resolve, reject);
        });
    };
    return Google;
}());
exports.Google = Google;
