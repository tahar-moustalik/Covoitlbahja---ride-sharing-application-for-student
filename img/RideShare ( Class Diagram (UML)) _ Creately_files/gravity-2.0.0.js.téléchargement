"use strict";

Object.defineProperty(exports, "__esModule", { value: true });
/**
 * class Screen
 * Contains functionality for getting
 * current viewport width and height.
 */
var Screen = /** @class */ (function () {
    function Screen() {
    }
    /**
     * Returns window width for
     * more standards compliant browsers (mozilla/netscape/opera/IE7)
     * @return number window width
     */
    Screen.prototype.getStandardBrowserWidth = function () {
        return window.innerWidth;
    };
    /**
     * Returns window height for
     * more standards compliant browsers (mozilla/netscape/opera/IE7)
     * @return number window height
     */
    Screen.prototype.getStandardBrowserHeight = function () {
        return window.innerHeight;
    };
    /**
     * Returns window width for
     * IE6 in standards compliant mode
     * with a valid doctype as the first line in the document
     * @return number window width
     */
    Screen.prototype.getStandardModeWidth = function () {
        return document.documentElement.clientWidth;
    };
    /**
     * Returns window height for
     * IE6 in standards compliant mode
     * with a valid doctype as the first line in the document
     * @return number window height
     */
    Screen.prototype.getStandardModeHeight = function () {
        return document.documentElement.clientHeight;
    };
    /**
     * Returns window width for
     * older versions of IE
     * @return number window width
     */
    Screen.prototype.getDefaultWidth = function () {
        return document.getElementsByTagName('body')[0].clientWidth;
    };
    /**
     * Returns window height for
     * older versions of IE
     * @return number window height
     */
    Screen.prototype.getDefaultHeight = function () {
        return document.getElementsByTagName('body')[0].clientHeight;
    };
    /**
     * Returns viewport width by detecting the browser
     * @return number viewport width
     */
    Screen.prototype.width = function () {
        var viewPortWidth;
        viewPortWidth = this.getStandardBrowserWidth();
        if (viewPortWidth === undefined) {
            viewPortWidth = this.getStandardModeWidth();
            if (viewPortWidth === undefined || viewPortWidth == 0) {
                viewPortWidth = this.getDefaultWidth();
            }
        }
        return viewPortWidth;
    };
    /**
     * Returns viewport height by detecting the browser
     * @return number viewport height
     */
    Screen.prototype.height = function () {
        var viewPortHeight;
        viewPortHeight = this.getStandardBrowserHeight();
        if (viewPortHeight === undefined) {
            viewPortHeight = this.getStandardModeHeight();
            if (viewPortHeight === undefined || viewPortHeight == 0) {
                viewPortHeight = this.getDefaultHeight();
            }
        }
        return viewPortHeight;
    };
    return Screen;
}());
exports.Screen = Screen;
