 */
var Token = /** @class */ (function () {
    /**
     * Set given Gravity token string to the instance and decode it for further functionality
     */
    function Token(token) {
        this.TOKEN_EXPIRATION_TIME_CLAIM = 'exp';
        this.TOKEN_USERID_CLAIM = 'uid';
        this.TOKEN_ISSUER_CLAIM = 'iss';
        this.TOKEN_ISSUER = 'creately.com';
