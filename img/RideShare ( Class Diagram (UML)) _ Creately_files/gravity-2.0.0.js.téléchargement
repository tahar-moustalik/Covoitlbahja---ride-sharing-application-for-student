/***/ (function(module, exports, __webpack_require__) {

"use strict";

Object.defineProperty(exports, "__esModule", { value: true });
var _ = __webpack_require__(75);
var envConfig = __webpack_require__(2);
window.fbAsyncInit = {};
/**
 * Facebook class
 * provide functionality to authenticate a user using Facebook
 * and get the accessToken related to the authenticated user.
 */
var Facebook = /** @class */ (function () {
    /**
     * Set the initiator function and load the js SDK
     */
    function Facebook() {
        var _this = this;
        this.sdkReady = new Promise(function (resolve) {
            _this.afterSDKIsReady = resolve;
        });
        /**
         * Set the initiation function to the global FB object, FB SDK will call the function
         * once the loading is complete
         */
        window.fbAsyncInit = function () {
            window.FB.init({
                appId: _this.getFacebookAppId(),
                status: true,
                xfbml: true,
                oauth: true,
                version: 'v2.9',
            });
            _this.afterSDKIsReady();
        };
        var d = document;
        var js;
        var id = 'facebook-jssdk';
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement('script');
        js.id = id;
        js.async = true;
        js.src = '//connect.facebook.net/en_US/all.js';
        d.getElementsByTagName('head')[0].appendChild(js);
    }
    /**
     * Read the Facebook app id from the config file and return it
     * @return Facebook app id
     */
    Facebook.prototype.getFacebookAppId = function () {
        return envConfig.facebook.appId;
    };
    /**
     * Initiate user authentication via Facebook
     * @param resolve resolve Promise function to call when resolved
     * @param reject reject Promise function to call when rejected
     */
    Facebook.prototype.initiateFacebook = function (resolve, reject) {
        var _this = this;
        this.resolveMethod = resolve;
        this.rejectMethod = reject;
        window.FB.getLoginStatus(function (response) {
            _this.action(response, function (loginResponse) { return _this.actionSuccess(loginResponse); }, function () { return _this.actionLogin(); });
        });
    };
    /**
     * Call successCallBack or failedCallBack functions based on response object conditions.
     * @param response FB login response
     * @param successCallBack function to call when login is complete
     * @param failedCallBack function to call when an error is returned on FB API call
     */
    Facebook.prototype.action = function (response, successCallBack, failedCallBack) {
        if (!response || response.error) {
            // There is no response from Facebook SDK method call or SDK returned an error
            this.actionFailed(response);
        }
        else if (response.status == 'connected' || response.email) {
            // User is already connected using Facebook OR
            // API call to get user email returned with user email
            successCallBack(response);
        }
        else {
            failedCallBack(response);
        }
    };
    /**
     * Function which resolves the promise.
     * return the Facebook access token of the user when the Promise is resolved.
     * @param response FB login response
     */
    Facebook.prototype.actionSuccess = function (response) {
        this.resolveMethod(response.authResponse.accessToken);
    };
    /**
     * Function which rejects the promise when an error happen
     * during user authentication with Facebook
     * @param error Error returned by FB API
     */
    Facebook.prototype.actionFailed = function (error) {
        this.rejectMethod(error);
    };
    /**
     * Log in the user using Facebook SDK. It is a must to
     * pass email scope to the login API call, otherwise we can not
     * get user email using the accessToken returned.
     */
    Facebook.prototype.actionLogin = function () {
        var _this = this;
        window.FB.login(function (response) {
            _this.action(response, function (loginResponse) { return _this.actionEmail(loginResponse); }, function (error) { return _this.actionFailed(error); });
        }, { scope: 'email' });
    };
    /**
     * Get the user email using the Facebook SDK. During user authorization with the
     * app, we MUST request access to usr email using 'email' scope. If not requested
     * Facebook will not return us the user email of current user.
     * @param response FB login response object
     */
    Facebook.prototype.actionEmail = function (response) {
        var _this = this;
        window.FB.api('/me', { fields: 'email' }, function (data) {
            data = _.extend(data, response);
            delete data.status;
            _this.action(data, function (apiResponse) { return _this.actionSuccess(apiResponse); }, function (error) { return _this.actionFailed(error); });
        });
    };
    /**
     * Return a Promise which will be resolved after the SDK is loaded
     */
    Facebook.prototype.ready = function () {
        return this.sdkReady;
    };
    /**
     * API function to authenticate a user using Facebook.
     * Will return a Promise.
     * @return Promise
     */
    Facebook.prototype.login = function () {
        var _this = this;
        return new Promise(function (resolve, reject) {
            _this.initiateFacebook(resolve, reject);
        });
    };
    return Facebook;
}());
exports.Facebook = Facebook;
